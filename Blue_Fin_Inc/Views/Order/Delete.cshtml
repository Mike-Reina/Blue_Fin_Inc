@model Blue_Fin_Inc.Models.Order

@{
    ViewData["Title"] = "Delete";
}



<h4>Are you sure you want to delete this order?</h4>
<div>
    <br>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CustomerName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CustomerName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Eircode)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Eircode)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ContactNo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContactNo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OrderPrice)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OrderPrice)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ContainsLivestock)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContainsLivestock)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="OrderNo" />
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>
<div>
    <a asp-action="Index">Back to Orders</a>
</div>
